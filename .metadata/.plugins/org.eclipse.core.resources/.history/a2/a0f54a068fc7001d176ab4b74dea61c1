package statements;

public class S003_SwitchCase {
	public static void main(String[] args) {
		
		
		int num =5;
		
		switch(num)
		{
		case 1:
			System.out.println("You have choese gujarati");
			break;
		case 2:
			System.out.println("You have choese Hindi");
			break;
		case 3:
			System.out.println("You have choese English");
			break;
			
		}
		
		
		
		
	}
	
	
	
}
